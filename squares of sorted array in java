
import java.util.*;
class SquaresofsortedArray{
public static void main(String args[]){
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();
int a[]=new int[n];
for(int i=0;i<n;i++){
a[i]=sc.nextInt();
}
for(int i=0;i<n;i++){
   a[i]=a[i]*a[i];
  }
Arrays.sort(a);
for(int i=0;i<n;i++){
System.out.println(a[i]);
}
}
}
sample input
5
-1 -4 0 3 10
sample output
0
1
9
16
100
